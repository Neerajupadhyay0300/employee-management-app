{
  "name": "server2",
  "version": "1.0.0",
  "main": "index.js",
"scripts": {
  "start": "nodemon --watch src --exec ts-node src/index.ts",
  "start:prod": "node dist/index.js",
  "build": "tsc",
  "postinstall": "prisma generate --schema src/prisma/schema.prisma",
  "migrate:dev": "prisma migrate dev --schema src/prisma/schema.prisma",
  "migrate:deploy": "prisma migrate deploy --schema src/prisma/schema.prisma",
  "migrate:status": "prisma migrate status --schema src/prisma/schema.prisma",
  "railway:deploy": "npm run migrate:deploy && npm run start:prod",
  "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.10.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "nodemon": "^3.1.11",
    "prisma": "^4.15.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
